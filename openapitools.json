{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.0.0",
    "generators": {
      "frontend": {
        "inputSpec": "./openapi.yaml",
        "output": "./src/openapi",
        "generatorName": "typescript-fetch",
        "generateAliasAsModel": true,
        "additionalProperties": {
          "fileNaming": "kebab-case",
          "generateAliasAsModel": true,
          "modelPropertyNaming": [
            "camelCase"
          ]
        }
      }
    }
  }
}